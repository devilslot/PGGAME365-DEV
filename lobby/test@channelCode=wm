	<script>
		document.title = "WM";
		window.co = "wm";
		window.version = "20.4.1.1";
		window.loginparams = window.location.hash.substr(1).split('&');
		window.location.hash = "";
		window.size = 1;
		window.lang = "cn";
		window.ui = 0;
		window.assignUI = false;
		window.register = 0;
		switch (document.domain)
		{
			case "demo.net":
				window.ui = 0;
				window.assignUI = true;
				break;
			default:

				break;
		}
		if (document.domain.split(".")[0] == "kposa01")
			window.ui = 1;

		var params = window.location.search.substr(1).split('&');
		params = params.concat(window.loginparams);
		var d = new Date();
		d.setTime(d.getTime() + (60 * 1000));
		for (var number in params)
		{
			var keyAndValue = params[number].split('=');
			var key = keyAndValue[0];
			var value = keyAndValue[1];
			switch(key)
			{
				case "debug":
					window.debug = value == "true";
					break;
				case "sid":
					document.cookie = "sid="+ value + "; " + "expires=" + d.toGMTString() + "; path=/"; 
					break;
				case "lang":
					document.cookie = "lang="+ value + "; path=/"; 
					break;
				case "size":
					window.size = value;
					break;
				default:
			}
		}
	</script>
