	<script type="text/javascript" src="cordova.js"></script>
	<script>
		document.querySelector("html").setAttribute("co", window.co);
		document.querySelector(".version-box").innerHTML = window.version;
		var params = window.location.search.substr(1).split('&');
		params = params.concat(window.loginparams);
		var d = new Date();
		d.setTime(d.getTime() + (60 * 1000));
		for (var number in params)
		{
			var keyAndValue = params[number].split('=');
			var key = keyAndValue[0];
			var value = keyAndValue[1];
			switch(key)
			{
				case "ui":
					window.assignUI = true;
					window.ui = parseInt(value);
					break;
				default:
			}
		}
		switch (window.ui)
		{
			case 1:
				document.getElementById("container").classList.add("qversion_box");
				document.getElementById("container").classList.remove("type2");

				document.getElementById("extend-container").classList.add("qversion_box");
				document.getElementById("extend-container").classList.remove("type2");
				break;
			case 2:
				document.getElementById("container").classList.add("qversion_box");
				document.getElementById("container").classList.add("type2");

				document.getElementById("extend-container").classList.add("qversion_box");
				document.getElementById("extend-container").classList.add("type2");
				break;
			default:
				document.getElementById("container").classList.remove("qversion_box");
				document.getElementById("container").classList.remove("type2");

				document.getElementById("extend-container").classList.remove("qversion_box");
				document.getElementById("extend-container").classList.remove("type2");
				break;
		}
		var domArr = document.querySelectorAll("#loading-box > *");
		for (var i = 0, len = domArr.length; i < len; i++)
		{
			domArr[i].style.display = "block";
		}

		if (window.co == "newmacau")
		{
			var dom = null;
			dom = document.getElementById("btn-menu");
			dom.parentNode.removeChild(dom);
			dom = document.getElementById("btn-changelanguage");
			dom.parentNode.removeChild(dom);
			dom = document.getElementById("btn-video");
			dom.parentNode.removeChild(dom);
		}
		var styleArr = null;
		var styleCount = 0;
		function OnLoad()
		{
			document.addEventListener('touchstart', (event) => 
			{
				if (event.touches.length > 1) 
				{
					event.preventDefault();
				}
			}, { passive: false });
			/*var lastTouchEnd = 0;
			document.addEventListener('touchend', (event) => 
			{
				const now = (new Date()).getTime();
				if (now - lastTouchEnd <= 300) 
					event.preventDefault();
				lastTouchEnd = now;
			}, false);*/
			
			var hiddenProperty = 'hidden' in document ? 'hidden' :    
				'webkitHidden' in document ? 'webkitHidden' :    
				'mozHidden' in document ? 'mozHidden' :    
				null;
			var visibilityChangeEvent = hiddenProperty.replace(/hidden/i, 'visibilitychange');
			var onVisibilityChange = function()
			{
				if (document[hiddenProperty]) 
				{
					try
					{
						main.pauseDateTime = new Date().getTime();
						sound.Pause();
						window.postMessage(
						{
							message:"VideoPlayerPause"
						}, "*");
					}
					catch (err) { }
				}
				else
				{
					try
					{
						if (window.navigator.standalone && main.resumeDateTime - main.pauseDateTime > 20000)
						{
							window.postMessage(
							{ 
								message: "LocationReload"
							}, "*");
						}
						else
						{
							main.resumeDateTime = new Date().getTime();
							sound.Resume();
							DoResize();
							window.postMessage(
							{
								message:"VideoPlayerResume"
							}, "*");
						}
					}
					catch (err) { }
				}
			}
			document.addEventListener(visibilityChangeEvent, onVisibilityChange);

			window.bApp = false;
			document.addEventListener("deviceready", function()
			{
				console.log("deviceready");

				//刪除直版按鈕
				var goToVerticalDom = document.getElementById("goToVertical");
				if (goToVerticalDom)
					goToVerticalDom.parentNode.removeChild(goToVerticalDom);

				document.removeEventListener(visibilityChangeEvent, onVisibilityChange);

				window.bApp = true;

				var gameIframeArr = document.querySelectorAll(".gameIframe");
				for (var i = 0; i < gameIframeArr.length; i++)
					gameIframeArr[i].style.position = "fixed";

				document.addEventListener("pause", function()
				{
					try
					{
						console.log("pause");
						main.pauseDateTime = new Date().getTime();
						sound.Pause();
						window.postMessage(
						{
							message:"VideoPlayerPause"
						}, "*");
					}
					catch (err) { }
				}, false);
				document.addEventListener("resume", function()
				{
					try
					{
						console.log("resume");
						main.resumeDateTime = new Date().getTime();
						if (main.resumeDateTime - main.pauseDateTime > 20000)
						{
							window.location = "index.html?sid=" + gData.myData.sid + "&bSidLogin=false";
						}
						else
						{
							sound.Resume();
							window.postMessage(
							{
								message:"VideoPlayerResume"
							}, "*");
						}
					}
					catch (err) { }
				}, false);
			}, false);

			var script = document.createElement('script');
			script.setAttribute('type', 'text/javascript');
			script.setAttribute('src', "./js/common.js?version=" + window.version);
			script.onload = function()
			{
				window.mainContainerDom = document.getElementById("container");
				window.mainExtendContainerDom = document.getElementById("extend-container");
				window.addEventListener("resize", DoResize);
				window.Scaling();

				document.getElementById("popup-box-container").style.display = "none";
				var script = document.createElement('script');
				script.setAttribute('type', 'text/javascript');
				script.setAttribute('src', "js/urlController.js" + "?version=" + window.version);
				// script.setAttribute('src', "../wm_js/js_original/urlController.js" + "?version=" + window.version);
				document.body.appendChild(script);
			};
			document.getElementsByTagName("head")[0].appendChild(script);
		}
		function DoResize()
		{
			clearTimeout(window.scalingStID);
			if (window.bFixedLandscape)
			{
				window.scalingStID = setTimeout(function()
				{
					window.Scaling();
					requestAnimationFrame(function()
					{
						window.Scaling();
						requestAnimationFrame(function()
						{
							window.Scaling();
						});
					});
				}, 1000);
			}
			else
			{
				window.scalingStID = setTimeout(function()
				{
					window.Scaling();
				}, 300);
			}
		}
		window.bScaling = true;
		window.setWidth = 1920;
		window.setHeight = 1080;
		window.bFixedLandscape = false;
		window.bPortrait = false;
		window.bRotate90 = false;
		window.maxScale = 16 / 7.5;	//最大壓縮高度
		window.aspectRatio = 16 / 9;
		function Scaling()
		{
			if (window.bScaling)
			{
				var width = window.innerWidth;
				var height = window.innerHeight;
				var top = 0;
				var left = 0;
				window.bRotate90 = (window.bFixedLandscape && common.bMobile && width < height) ? true : false;
				if (navigator.userAgent.indexOf("MiuiBrowser") >= 0 && window.bRotate90 == false) 	//fix安卓原生瀏覽器bug 下方擋住
					height *= 0.85;

				if (window.bFixedLandscape || width > height)
				{
					var ww = width;
					var wh = height;
					var rate = window.setWidth / window.setHeight;
					if (common.bMobile)
					{
						if (ww > wh)
							rate = ww / wh > window.maxScale ? window.maxScale : ww / wh;
						else
							rate = wh / ww > window.maxScale ? window.maxScale : wh / ww;
					}
					var w1 = ww,
						h1 = Math.round(w1 / rate),
						h2 = wh,
						w2 = Math.round(h2 * rate);

					width = (w1 < w2) ? w1 : w2;
					height = (w1 < w2) ? h1 : h2;
					top = (wh - height) / 2;
					left = (ww - width) / 2;
					var nowScaleX = width / window.setWidth;
					var nowScaleY = height / window.setHeight;
				}
				window.aspectRatio = width / height;
				if (window.aspectRatio < 1 && window.bRotate90 == false)
					window.bPortrait = true;
				else
					window.bPortrait = false;

				if (window.bRotate90)
				{
					document.body.style.transform = "rotate(90deg)";
					var temp = width;
					width = temp * rate < wh ? temp * rate : wh;
					height = temp < width / rate ? temp : width / rate;
					top = -height - (ww - height) / 2;
					left = (wh - width) / 2;
				}
				else
				{
					document.body.style.transform = null;
				}

				document.documentElement.style.fontSize = (width / 100) + "px";
				mainContainerDom.style.top = top + "px";
				mainContainerDom.style.left = left + "px";
				mainContainerDom.style.width = width + "px";
				mainContainerDom.style.height = height + "px";
				mainExtendContainerDom.style.top = top + "px";
				mainExtendContainerDom.style.left = left + "px";
				mainExtendContainerDom.style.width = width + "px";
				mainExtendContainerDom.style.height = height + "px";

				if (window.bApp == false && window.size != 1)
				{
					document.getElementsByTagName("html")[0].style.width = width + "px";
					document.getElementsByTagName("html")[0].style.height = (height * 1.4) + "px";
				}
				if (window.main)
					window.main.Scaling(ww, wh, width, height, top, left, window.bPortrait, window.bRotate90);
			}
			if (!window.main || window.main.nowGameID == 0)
			{
				clearTimeout(window.scalingStID);
				window.scalingStID = setTimeout(function()
				{
					window.Scaling();
				}, 1000);
			}
		}
	</script>
</body>
</html>